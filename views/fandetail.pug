extends layout 
 
block content 
  h1= title 
 
  - let fanid = toShow._id 
  - let fanbrand = toShow.brand
  - let fan_type = toShow.types 
  - let fancost = toShow.cost 
  
  div 
    div.row 
      div(class='fanAttr mr-3') fan ID is: 
      div #{fanid} 
    div.row 
      div(class='fanAttr mr-3') fan Cost is: 
      div #{fanbrand}
    div.row 
      div(class='fanAttr mr-3') fan Type is: 
      div #{fan_type} 
    div.row 
      div(class='fanAttr mr-3') fan Cost is: 
      div #{fancost} 